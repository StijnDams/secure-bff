{
	"name": "hono-session-middleware",
  "main": "src/index.ts",
	"scripts": {
		"dev": "bun run --hot src/index.ts",
		"dev:debug": "bun run --hot --inspect src/index.ts",
		"db:gen": "drizzle-kit generate --dialect sqlite --schema ./src/models",
		"db:migrate": "drizzle-kit migrate",
		"db:init": "bun db:gen && bun db:migrate"
	},
	"dependencies": {
		"drizzle-orm": "^0.38.3",
		"hono": "^4.6.16",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@types/bun": "^1.1.16",
		"@types/node": "^20.11.5"
	}
}
